<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kaishengit.crm.mapper.SalechancerecordMapper">
  <update id="saveNewSalechancerecord">
      UPDATE salechancerecord
      SET sale_id = #{saleId},
       create_time = #{createTime},
       content = #{content}
  </update>


</mapper>